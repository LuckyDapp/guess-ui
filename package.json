{
  "name": "guess-the-number-ui",
  "version": "1.0.0",
  "description": "Guess The Number UI",
  "license": "Apache-2.0",
  "main": "src/index.tsx",
  "scripts": {
    "clean": "rm -rf dist .papi",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "gen-pah-rpc": "yarn papi add -w wss://westend-asset-hub-rpc.polkadot.io pah",
    "gen-ink-v6-contract-metadata": "yarn papi ink add metadata/guess_the_number_westend.json",
    "generate": "yarn gen-pah-rpc && yarn gen-ink-v6-contract-metadata",
    "postinstall": "yarn generate",
    "find-tx": "node src/scripts/find-tx.mjs",
    "decode-tx": "node src/scripts/decode-tx.mjs",
    "decode-contract-event": "node src/scripts/decode-contract-event.mjs",
    "scan-block": "node src/scripts/scan-blocks.mjs",
    "scan-range": "node src/scripts/scan-blocks.mjs",
    "test-block": "node src/scripts/test-block-events.mjs",
    "test-contract": "node src/scripts/test-contract-query.mjs"
  },
  "peerDependencies": {
    "polkadot-api": "*"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@polkadot-api/descriptors": "file:.papi/descriptors",
    "@polkadot-api/sdk-ink": "^0.6.2",
    "@polkadot/api": "^12.4.2",
    "@polkadot/keyring": "^13.5.6",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@reactive-dot/react": "^0.46.2",
    "@types/three": "^0.180.0",
    "dayjs": "^1.11.18",
    "dot-connect": "^0.22.1",
    "polkadot-api": "^1.23.1",
    "react": "latest",
    "react-dom": "latest",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.5.2",
    "rxjs": "^7.8.0",
    "three": "^0.180.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "latest",
    "vite": "latest"
  }
}
